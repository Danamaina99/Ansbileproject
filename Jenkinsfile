#!groovy

def version = "v.13, v.14"

pipeline {
    agent any
    
    parameters {
        
        choice(Name: version, Choices: v.13, v.14, Description: versions needs to release
    
   
      
    stages {
      stage('checkout') {
           steps {
             
                git  'https://github.com/devops4solutions/CI-example.git'
             
          }
        }
      }
      }
     
